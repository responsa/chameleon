<?xml version="1.0" encoding="UTF-8"?>
<root>
  <% @widget.y_axis.each do |item| %>
    <item><%= item %></item>
  <% end %>
  <settings>
    <% @widget.x_axis.each do |x| %>
      <axisx><%= x %></axisx>
    <% end %>
    <% @widget.y_axis_labels.each do |y| %>
      <axisy><%= y %></axisy>
    <% end %>
    <colour><%= @widget.colour %></colour>
  </settings>
</root>
